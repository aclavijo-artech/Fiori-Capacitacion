{
    "_version": "1.65.0",
    "sap.app": {
        "id": "clase3",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.1",
            "toolsId": "28a0febe-27f8-453f-89a6-abc32a7934ab"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.139.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "clase3.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "clase3.view",
                "async": true
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": ":?query:",
                    "target": [
                        "TargetApp"
                    ]
                },
                {
                    "name": "RouteSecondPage",
                    "pattern": "SecondPage",
                    "target": [
                        "TargetSecondPage"
                    ]
                },
                {
                    "name": "RouteThirdPage",
                    "pattern": "ThirdPage/{param}",
                    "target": [
                        "TargetThirdPage"
                    ]
                },
                {
                    "name": "RouteFourthPage",
                    "pattern": "ThirdPage/{param}/FourthPage",
                    "target": [
                        "TargetFourthPage"
                    ]
                }
            ],
            "targets": {
                "TargetApp": {
                    "id": "App",
                    "name": "App"
                },
                "TargetSecondPage": {
                    "id": "SecondPage",
                    "name": "SecondPage"
                },
                "TargetThirdPage": {
                    "id": "ThirdPage",
                    "name": "ThirdPage"
                },
                "TargetFourthPage": {
                    "id": "FourthPage",
                    "name": "FourthPage"
                }
            }
        },
        "rootView": {
            "viewName": "clase3.view.App",
            "type": "XML",
            "id": "AppRoot"
        }
    }
}